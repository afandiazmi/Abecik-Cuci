{
  "manifest_version": 3,
  "name": "Clean Site Data",
  "version": "1.1",
  "description": "Clicks a button, waits, then cleans all site data of the current website.",
  "permissions": [
    "browsingData",
    "tabs",
    "notifications",
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_title": "Clean Site Data",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
